            <!-- wfa504  Input   -->
            <androidx.cardview.widget.CardView
                android:id="@+id/fldGrpCVwfa504"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_wfa504"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_wfa504"
                            android:text="@string/wfa504" />

                        <TextView
                            android:id="@+id/q_wfa504"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_wfa504"
                            android:layout_alignBottom="@id/qtxt_wfa504"
                            android:text="@string/Q_wfa504" />
                    </RelativeLayout>

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/wfa504"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="##.#"
                        android:inputType="number"
                        app:mask="##.#"
                        app:maxValue="41.0"
                        app:minValue="33.0"
                        app:pattern="^(\\d{2,2}\\.\\d{1,1})$"
                        app:type="range" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>


