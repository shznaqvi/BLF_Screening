                android:id="@+id/fldGrpCVwfa101"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_wfa101"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_wfa101"
                            android:text="@string/wfa101" />

                        <TextView
                            android:id="@+id/q_wfa101"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_wfa101"
                            android:layout_alignBottom="@id/qtxt_wfa101"
                            android:text="@string/Q_wfa101" />

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/wfa101"
                            android:layout_width="0dp"
                            android:layout_height="56dp"
                            android:layout_marginBottom="12dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:hint="###-##-##"
                            android:inputType="number"
                            app:mask="###-##-##"
                            app:required="true" />

                        <ImageButton
                            android:id="@+id/checkMR"
                            android:layout_width="56dp"
                            android:layout_height="56dp"
                            android:background="@drawable/et_style"
                            android:onClick="FetchFollowups"
                            android:src="@drawable/ic_check"
                            android:textSize="20sp" />

                        <ProgressBar
                            android:id="@+id/pbarMR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="gone" />


                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>


